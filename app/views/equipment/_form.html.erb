<%= simple_form_for(@equipment) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :description %>
    <%= f.input :condition %>
    <%= f.input :photo %>
    <%= f.input :estimated_value %>
    <%= f.association :category %>
    <%= f.input :purchase_date %>
    <%= f.input :city %>
    <%= f.input :state %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
